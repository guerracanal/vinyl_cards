{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "buildCommand": "apt-get update && xargs -a packages.txt apt-get install -y"

      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production",
      "FLASK_APP": "app.py",
      "requirements": "requirements.txt"
    }
  }